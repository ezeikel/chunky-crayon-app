{
  "name": "chunky-crayon",
  "main": "expo-router/entry",
  "version": "0.1.0",
  "scripts": {
    "clean": "git clean -xdf node_modules",
    "start": "expo start",
    "dev": "expo start --ios",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "prebuild:ios": "expo prebuild --platform ios --clean",
    "prebuild:android": "expo prebuild --platform android --clean",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "build:ios": "pnpm expo prebuild --platform ios --clean && pnpm ios",
    "build:android": "pnpm expo prebuild --platform android --clean && pnpm android",
    "build": "expo export",
    "eas:build:local": "eas build --profile development --local",
    "eas:build:local:android": "eas build --platform android --profile preview --local",
    "eas:build:development": "eas build --profile development",
    "eas:build:development:android": "eas build --platform android --profile development",
    "eas:build:preview": "eas build --profile preview",
    "eas:build:preview:ios": "eas build --profile preview --platform ios",
    "eas:build:preview:android": "eas build --profile preview --platform android",
    "eas:build:preview:ios:local": "eas build --profile preview --platform ios --local",
    "eas:build:preview:android:local": "eas build --profile preview --platform android --local",
    "eas:build:production": "eas build --profile production",
    "eas:submit:beta": "eas submit --profile beta",
    "eas:submit:production": "eas submit --profile production",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "check-types": "tsc --noEmit",
    "env:pull": "eas env:pull --environment development"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
    "@fortawesome/fontawesome-svg-core": "^6.7.1",
    "@fortawesome/free-solid-svg-icons": "^6.7.1",
    "@fortawesome/pro-light-svg-icons": "^6.7.1",
    "@fortawesome/pro-regular-svg-icons": "^6.7.1",
    "@fortawesome/pro-solid-svg-icons": "^6.7.1",
    "@fortawesome/react-native-fontawesome": "^0.3.2",
    "@react-navigation/bottom-tabs": "^7.0.14",
    "@react-navigation/native": "^7.0.9",
    "@sentry/react-native": "~6.14.0",
    "@shopify/flash-list": "1.7.6",
    "@shopify/react-native-skia": "2.0.0-next.4",
    "@tanstack/react-query": "^5.62.0",
    "axios": "^1.7.8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "expo": "~53.0.22",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-insights": "~0.9.3",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-print": "~14.1.4",
    "expo-router": "~5.1.5",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.2.0",
    "formik": "^2.4.6",
    "prettier": "^3.4.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-html-to-pdf": "^0.12.0",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "twrnc": "^4.6.0",
    "zod": "^3.23.8",
    "zod-formik-adapter": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@tanstack/eslint-plugin-query": "^5.61.6",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.14",
    "@types/react-test-renderer": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.10",
    "react-test-renderer": "18.3.1",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.8.3"
  },
  "private": true
}
